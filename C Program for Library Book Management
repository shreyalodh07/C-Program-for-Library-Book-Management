#include <stdio.h>
int main() {
    int choice;
    printf("1. Issue Book\n2. Return Book\n");
    printf("Enter choice: ");
    scanf("%d", &choice);
    if (choice == 1)
        printf("Book Issued");
    else if (choice == 2)
        printf("Book Returned");
    else
        printf("Invalid Choice");
    return 0;
}
